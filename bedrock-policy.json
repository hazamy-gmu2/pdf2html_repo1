{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "bedrock:InvokeDataAutomationAsync",
        "bedrock:GetDataAutomationResult",
        "bedrock:ListDataAutomations",
        "bedrock:CancelDataAutomation",
        "bedrock:InvokeModel",
        "bedrock-data-automation:*",
        "bedrock-data-automation-runtime:*"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket",
        "s3:ListObjects",
        "s3:ListObjectsV2"
      ],
      "Resource": [
        "arn:aws:s3:::shaashvat-pdf2html",
        "arn:aws:s3:::shaashvat-pdf2html/*"
      ]
    }
  ]
}
